{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["@typescript-eslint"],
  "globals": {
    "React": "writable",
    "WEB_URL": true,
    "require": false,
    "process": false
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
